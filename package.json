{
  "name": "sandbox",
  "version": "0.0.1",
  "description": "A place to play, learn, and build castles.",
  "keywords": [
    "html",
    "css",
    "javascript",
    "web development",
    "react",
    "frontend",
    "web design"
  ],
  "author": "Nick Garcia <me@nickgarcia.me> (https://nickgarcia.me)",
  "homepage": "https://github.com/NicksIdeaEngine/sandbox",
  "main": "./src/index.js",
  "license": "MIT",
  "private": true,
  "scripts": {
    "clean-dist": "rimraf dist/*",
    "clean-public": "rimraf public/*",
    "clean-lib": "rimraf src/lib/*",
    "clean-components": "rimraf src/components/*",
    "clean-all": "npm-run-all clean-dist clean-public clean-lib clean-components",
    "predev": "npm run clean-dist",
    "dev": "webpack --env.development --config webpack.config.js",
    "prebuild": "npm run clean-dist",
    "build": "webpack --env.production --config webpack.config.js",
    "devserver": "webpack-dev-server --env.development --config webpack.config.js",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/NicksIdeaEngine/sandbox.git"
  },
  "bugs": {
    "url": "https://github.com/NicksIdeaEngine/sandbox/issues",
    "email": "me@nickgarcia.me"
  },
  "prettier": {
    "arrowParens": "always",
    "singleQuote": true,
    "trailingComma": "all"
  },
  "browserslist": {
    "production": [">0.5%", "not dead", "last 2 versions"],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@emotion/core": "^10.0.28",
    "@emotion/styled": "^10.0.27",
    "core-js": "^3.6.5",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.9.4",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^25.5.1",
    "babel-loader": "^8.1.0",
    "babel-plugin-emotion": "^10.0.33",
    "css-loader": "^3.5.3",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-emotion": "^10.0.27",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^23.10.0",
    "eslint-plugin-jest-formatting": "^1.2.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^3.0.0",
    "html-webpack-plugin": "^4.3.0",
    "jest": "^25.5.4",
    "localtunnel": "^2.0.0",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "react-test-renderer": "^16.13.1",
    "rimraf": "^3.0.2",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.2.1",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0"
  }
}
