{
  "name": "sandbox",
  "version": "0.0.1",
  "description": "A place to play and learn.",
  "keywords": [
    "frontend",
    "web development",
    "javascript",
    "react"
  ],
  "author": "Nick Garcia <me@nickgarcia.me> (https://nickgarcia.me)",
  "homepage": "https://github.com/NicksIdeaEngine/sandbox",
  "main": "./src/index.js",
  "license": "MIT",
  "private": true,
  "scripts": {
    "clean-dist": "rimraf dist/*",
    "clean-components": "rimraf components/*",
    "clean-lib": "rimraf lib/*",
    "clean-public": "rimraf public/*",
    "clean-all": "npm-run-all clean-dist clean-components clean-lib clean-public",
    "predev": "npm run clean-dist",
    "dev": "webpack --env.development --config webpack.config.js",
    "prebuild": "npm run clean-dist",
    "build": "webpack --env.production --config webpack.config.js",
    "devserver": "webpack-dev-server --env.development --config webpack.config.js",
    "test": "jest",
    "localtunnel": "lt --port 8080 --subdomain 'smolapp'",
    "share": "run-s devserver localtunnel",
    "prestart": "npm run clean-dist",
    "start": "npm run devserver"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/NicksIdeaEngine/sandbox.git"
  },
  "bugs": {
    "url": "https://github.com/NicksIdeaEngine/sandbox/issues",
    "email": "me@nickgarcia.me"
  },
  "dependencies": {
    "@emotion/core": "^10.0.28",
    "@emotion/styled": "^10.0.27",
    "core-js": "^3.6.5",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.3",
    "@babel/core": "^7.10.3",
    "@babel/preset-env": "^7.10.3",
    "@babel/preset-react": "^7.10.1",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^25.5.1",
    "babel-loader": "^8.1.0",
    "babel-plugin-emotion": "^10.0.33",
    "browserslist": "^4.12.0",
    "css-loader": "^3.6.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-emotion": "^10.0.27",
    "eslint-plugin-html": "^6.0.2",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-jest": "^23.13.2",
    "eslint-plugin-jest-formatting": "^1.2.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^3.0.0",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.3.0",
    "jest": "^25.5.4",
    "localtunnel": "^2.0.0",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "react-test-renderer": "^16.13.1",
    "rimraf": "^3.0.2",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.2.1",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  }
}
