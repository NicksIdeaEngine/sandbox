{
  "name": "sandbox",
  "version": "0.0.7",
  "description": "A place to play and learn. This is a JS development environment used for testing and creating anything I do that doesn't need to be its own project.",
  "keywords": [
    "html",
    "css",
    "javascript",
    "web development",
    "react",
    "frontend",
    "web design"
  ],
  "author": "Nick Garcia <me@nickgarcia.me> (https://nickgarcia.me)",
  "homepage": "https://github.com/NicksIdeaEngine/sandbox",
  "main": "./src/index.js",
  "license": "MIT",
  "private": true,
  "scripts": {
    "clean-dist": "rimraf dist/*",
    "clean-public": "rimraf public/*",
    "predev": "npm run clean-dist",
    "dev": "webpack --env.development --config webpack.config.js",
    "prebuild": "npm run clean-dist",
    "build": "webpack --env.production --config webpack.config.js",
    "devserver": "webpack-dev-server --env.development --config webpack.config.js",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/NicksIdeaEngine/sandbox.git"
  },
  "bugs": {
    "url": "https://github.com/NicksIdeaEngine/sandbox/issues",
    "email": "me@nickgarcia.me"
  },
  "prettier": {
    "arrowParens": "always",
    "singleQuote": true,
    "trailingComma": "all"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "last 2 versions"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@emotion/core": "^10.0.28",
    "@emotion/styled": "^10.0.27",
    "core-js": "^3.6.5",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-react": "^7.9.4",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^25.3.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-emotion": "^10.0.33",
    "css-loader": "^3.5.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-loader": "^3.0.4",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-emotion": "^10.0.27",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-jest-formatting": "^1.2.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^1.7.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^25.3.0",
    "node-sass": "^4.13.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "react-test-renderer": "^16.13.1",
    "rimraf": "^3.0.2",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.4",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  }
}
