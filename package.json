{
  "name": "sandbox",
  "version": "0.0.1",
  "description": "A place to play and learn.",
  "keywords": [
    "frontend",
    "web development",
    "javascript",
    "react"
  ],
  "author": "Nick Garcia <me@nickgarcia.me> (https://nickgarcia.me)",
  "homepage": "https://github.com/NicksIdeaEngine/sandbox",
  "main": "./src/index.html",
  "license": "MIT",
  "private": true,
  "scripts": {
    "clean-components": "rimraf src/components/*",
    "clean-dist": "rimraf dist/*",
    "clean-lib": "rimraf src/lib/*",
    "clean-public": "rimraf public/*",
    "clean-utils": "rimraf src/utils/*",
    "clean-all": "npm-run-all clean-components clean-dist clean-lib clean-public clean-utils",
    "predev": "npm run clean-dist",
    "dev": "webpack --env.development --config webpack.config.js",
    "prebuild": "npm run clean-dist",
    "build": "webpack --env.production --config webpack.config.js",
    "devserver": "webpack-dev-server --env.development --config webpack.config.js",
    "test": "jest",
    "localtunnel": "lt --port 8080 --subdomain 'smolapp'",
    "share": "run-s devserver localtunnel",
    "prestart": "npm run clean-dist",
    "start": "npm run devserver"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/NicksIdeaEngine/sandbox.git"
  },
  "bugs": {
    "url": "https://github.com/NicksIdeaEngine/sandbox/issues",
    "email": "me@nickgarcia.me"
  },
  "dependencies": {
    "@emotion/core": "*10.1.1",
    "@emotion/styled": "*10.0.27",
    "core-js": "*3.7.0",
    "prop-types": "*15.7.2",
    "react": "*16.14.0",
    "react-dom": "*16.14.0"
  },
  "devDependencies": {
    "@babel/cli": "*7.12.1",
    "@babel/core": "*7.12.3",
    "@babel/preset-env": "*7.12.1",
    "@babel/preset-react": "*7.12.5",
    "babel-eslint": "*10.1.0",
    "babel-jest": "*25.5.1",
    "babel-loader": "*8.1.0",
    "babel-plugin-emotion": "*10.0.33",
    "browserslist": "*4.14.6",
    "css-loader": "*3.6.0",
    "eslint": "*6.8.0",
    "eslint-config-airbnb": "*18.2.1",
    "eslint-config-prettier": "*6.15.0",
    "eslint-loader": "*4.0.2",
    "eslint-plugin-babel": "*5.3.1",
    "eslint-plugin-emotion": "*10.0.27",
    "eslint-plugin-html": "*6.1.0",
    "eslint-plugin-import": "*2.22.1",
    "eslint-plugin-jest": "*23.20.0",
    "eslint-plugin-jest-formatting": "*1.2.0",
    "eslint-plugin-jsx-a11y": "*6.4.1",
    "eslint-plugin-prettier": "*3.1.4",
    "eslint-plugin-react": "*7.21.5",
    "eslint-plugin-react-hooks": "*3.0.0",
    "file-loader": "*6.2.0",
    "html-webpack-plugin": "*4.5.0",
    "jest": "*25.5.4",
    "localtunnel": "*2.0.0",
    "node-sass": "*4.14.1",
    "npm-run-all": "*4.1.5",
    "prettier": "*2.1.2",
    "react-test-renderer": "*16.14.0",
    "rimraf": "*3.0.2",
    "sass-loader": "*8.0.2",
    "style-loader": "*1.3.0",
    "webpack": "*4.44.2",
    "webpack-cli": "*3.3.12",
    "webpack-dev-server": "*3.11.0"
  }
}
